<div>
    <h2> <%= link_to article.title, article_path(article.id) %> </h2>
    <p> <%= article.created_at.strftime('%Y/%m/%d') %> </p>
    <p>
        <% if user_signed_in? %>
            <%= link_to '編集', edit_article_path(article.id) %>
            <%= link_to '削除', article_path(article.id), method: :delete %>
        <% end %>    
            
    </p>    
</div>